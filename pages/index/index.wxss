/* 轮播图样式 */
.banner{
  width: 100%;
  height: 300rpx;
}
.banner swiper-item image{
  width: 100%;
  height: 100%;
}

/* 五个图标文字样式 */
.navContainer{
  display: flex;
  flex-direction: row;
}
.navContainer .navItem{
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 20%;
}
.navContainer .navItem .iconfont{
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
  border-radius: 50%;
  background-color: rgb(240, 19, 19);
  color: #fff;
  margin: 20rpx 0;
  font-size: 50rpx;
}
.navItem text{
  font-size: 24rpxx;
}